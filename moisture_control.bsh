while [ 1 ]
do
   for i in {1..60}
   do
       echo "executing moisture control $i"
       /usr/bin/python moisture_control.py  >>  /tmp/moisture_control.err   2>&1
     
        mv /tmp/moisture_control.err /tmp/moisture_control.errr
        sleep 5
   done
   python cloud_event_queue.py "reboot" "moisture_control"
done
